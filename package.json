{
  "name": "targid_dummy",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/targid_dummy",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_data_sql": "*",
    "targid2": "*"
  },
  "caleydo": {
    "plugins": {
      "web": [
        {
          "id": "dummy_start_a",
          "name": "Dummy A",
          "type": "targidView",
          "file": "DummyList",
          "factory": "createStartA",
          "idtype": "IDTypeA",
          "selection": "none"
        },{
          "id": "dummy_start_a",
          "name": "Dummy A",
          "type": "targidStartEntryPoint",
          "file": "DummyEntryPoint",
          "factory": "createStartFactory",
          "viewId": "dummy_start_a",
          "idtype": "IDTypeA"
        },
        {
          "id": "dummy_start_B",
          "name": "Dummy B",
          "type": "targidView",
          "file": "DummyList",
          "factory": "createStartB",
          "idtype": "IDTypeB",
          "selection": "none"
        },
        {
          "id": "dummy_detail",
          "name": "Dummy Detail View",
          "type": "targidView",
          "category": "static",
          "file": "DummyDetailView",
          "idtype": "IDTypeA",
          "selection": "multiple"
        },
        {
          "id": "dummy_score",
          "name": "Dummy Score",
          "type": "targidView",
          "category": "dynamic",
          "file": "DummyDependentList",
          "factory": "create",
          "idtype": "IDTypeA",
          "selection": "single"
        },
        {
          "id": "dummy_external",
          "name": "DuckDuckGo",
          "type": "targidView",
          "category": "static",
          "site": "https://duckduckgo.com/?q={id}",
          "argument": "id",
          "idtype": "IDTypeA",
          "selection": "single"
        }
      ],
      "python": [
        {
          "type": "targid-sql-database-definition",
          "id": "dummy",
          "configKey": "targid_dummy"
        }
      ]
    },
    "products": [
      {
        "name": "TargidDummy",
        "type": "python",
        "app": "targid2",
        "plugins": [
          "targid2",
          "targid_dummy",
          "caleydo_server"
        ]
      }
    ]
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
